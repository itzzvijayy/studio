rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    /**
     * VISION-AI CLEAN MADURAI - SECURITY RULES
     * Updated to allow broad access for authenticated users to resolve listing issues.
     * Deployment Trigger: 2024-05-22T14:45:00Z
     */
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}